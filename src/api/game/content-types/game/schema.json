{
  "kind": "collectionType",
  "collectionName": "games",
  "info": {
    "singularName": "game",
    "pluralName": "games",
    "displayName": "game"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "is_global": {
      "type": "boolean",
      "default": true
    },
    "name": {
      "type": "string",
      "maxLength": 12
    },
    "description": {
      "type": "text",
      "maxLength": 4000
    },
    "is_popular": {
      "type": "boolean",
      "default": false
    },
    "images": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "item": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::voucher.voucher",
      "inversedBy": "game"
    }
  }
}
